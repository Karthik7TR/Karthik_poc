				<bos/>
				<headtext>Agnor&apos;s Georgia Evidence</headtext>
				<eos/>
				<eop/>
			</prop.head>
			<content.metadata.block>
				<cmd.dates>
					<cmd.currency.default iso.d="20191101">
						<bop/>
						<bos/>November 2019 Update<eos/>
