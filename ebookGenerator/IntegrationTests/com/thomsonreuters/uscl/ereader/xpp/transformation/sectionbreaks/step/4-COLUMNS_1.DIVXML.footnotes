							<t style="ref.1 cgt">§ </t>
							<XPPLink guid='I211e909797f011de90379de361181c6d'>
								<t style="ref.1">19:22</t>
							</XPPLink>
						</ref>
					</cite>
					<t style="para.text.2">, </t>
					<cite.query w-seq-number="02215" w-ref-type="RT" w-serial-number="1977134604" w-pub-number="0000360" ID="I845cbaf017d611e891d0ff925e0903bf">
