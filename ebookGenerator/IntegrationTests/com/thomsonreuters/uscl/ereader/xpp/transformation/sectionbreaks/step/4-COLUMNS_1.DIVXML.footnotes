		</footnote.reference>
		<footnote.body date.updated="0">
			<para date.updated="0" first.para="yes">
				<para.text date.updated="0" first.para.text="yes">
					<cite>
						<ref case.id="2011629156" date="2007" fjur="Ga" jur="Ga" norm.rptr="gaapp" p1="Williams" p2="Martin Lakes Condominium Association, Inc." post.rptr="569" pre.rptr="284" ref.id="If30d8bb0c6ac11dfb3080000837bc6dd" title="Williams v. Martin Lakes Condominium Association, Inc.">
							<cite.query w-seq-number="00833" w-ref-type="RP" w-serial-number="2011629156" w-pub-number="0000711" ID="I361312e217d611e891d0ff925e0903bf">
