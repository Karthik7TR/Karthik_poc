			<t style="footnote.reference super cgt">5</t>
		</footnote.reference>
		<footnote.body date.updated="0">
			<para date.updated="0" first.para="yes">
				<para.text date.updated="0" first.para.text="yes">
					<cite>
						<ref normalpub="statute-us-ga-code" pararef="(26)" ref.id="Ib682e7f07dc311ddbffc859d7d7fb5fd" secref="10-5-2" subref="(a)">
							<cite.query w-seq-number="01549" w-ref-type="LQ" w-normalized-cite="GAST10-5-2" w-pub-number="1000468" ID="I6329479017d611e891d0ff925e0903bf">
								<t style="ref.1">O.C.G.A. </t>
								<t style="ref.1 cgt">§ </t>
								<t style="ref.1">10-5-2(a)(26)</t>
							</cite.query>
						</ref>
