			<para date.updated="0" first.para="no">
				<para.text date.updated="0" first.para.text="yes">
					<t style="para.text.2">The court in </t>
					<cite.query w-seq-number="01995" w-ref-type="RS" w-serial-number="1995148548" w-pub-number="0000711" ID="I7c31aa7217d611e891d0ff925e0903bf">
