						<ref case.id="1966133038" date="1966" fjur="Ga" jur="Ga" norm.rptr="gaapp" p1="Millholland" p2="Oglesby" pin.point="747&amp;#8211;748" post.rptr="745" pre.rptr="114" ref.id="Ifca65d00c6ac11dfbba20000837bc6dd" title="Millholland v. Oglesby">
							<cite.query w-seq-number="02346" w-ref-type="RP" w-serial-number="1966133038" w-pub-number="0000711" ID="I896f67e517d611e891d0ff925e0903bf">
