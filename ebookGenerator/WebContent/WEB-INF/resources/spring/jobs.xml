		</step>
		<step id="sj1_Step2">
			<tasklet ref="stubTask"/>
			<next on="COMPLETED" to="sj1_Step3"/>
		</step>
		<step id="sj1_Step3">
			<tasklet ref="stubTask"/>
		</step>
	</job>
	
	<job id="stubJob2">
		<step id="sj2_Step0">
			<tasklet ref="stubTask"/>
			<next on="COMPLETED" to="sj2_Step1"/>
		</step>
		<step id="sj2_Step1">
