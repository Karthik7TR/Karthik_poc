		</step>
	</job>	
	
	<job id="stubJob3">
		<step id="sj3_Step0">
			<tasklet ref="stubTask"/>
			<next on="COMPLETED" to="sj3_Step1"/>
		</step>
		<step id="sj3_Step1">
			<tasklet ref="stubTask"/>
			<next on="COMPLETED" to="sj3_Step2"/>
