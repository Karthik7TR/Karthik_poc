<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
						   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd
        				   http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.0.xsd">

	<bean id="bookDefinitionService"
		class="com.thomsonreuters.uscl.ereader.core.book.service.BookDefinitionServiceImpl"
		p:bookDefinitionDao-ref="bookDefinitionDao" />

	<bean id="stateCodeService"
		class="com.thomsonreuters.uscl.ereader.core.book.statecode.StateCodeServiceImpl"
		p:stateCodeDao-ref="stateCodeDao" />
	<bean id="codeService"
		class="com.thomsonreuters.uscl.ereader.core.book.service.CodeServiceImpl"
		p:codeDao-ref="codeDao" />

	<bean id="authorService"
		class="com.thomsonreuters.uscl.ereader.core.book.service.AuthorServiceImpl"
		p:authorDAO-ref="authorDao" />

	<bean id="pilotBookService"
		class="com.thomsonreuters.uscl.ereader.core.book.service.PilotBookServiceImpl"
		p:pilotBookDAO-ref="pilotBookDao" />

	<bean id="eBookAuditService"
		class="com.thomsonreuters.uscl.ereader.core.book.service.EBookAuditServiceImpl"
		p:eBookAuditDAO-ref="eBookAuditDao" />

	<bean id="proviewAuditService"
		class="com.thomsonreuters.uscl.ereader.proviewaudit.service.ProviewAuditServiceImpl"
		p:proviewAuditDao-ref="proviewAuditDao" />

	<bean id="bookDefintionLockService"
		class="com.thomsonreuters.uscl.ereader.core.book.service.BookDefinitionLockServiceImpl"
		p:bookDefinitionLockDao-ref="bookDefintionLockDao" />

	<bean id="publishingStatsService"
		class="com.thomsonreuters.uscl.ereader.stats.service.PublishingStatsServiceImpl"
		p:publishingStatsDAO-ref="publishingStatsDao" 
		p:publishingStatsUtil-ref="publishingStatsUtil"/>

	<bean id="jobService"
		class="com.thomsonreuters.uscl.ereader.core.job.service.JobServiceImpl"
		p:jobDao-ref="jobDao" p:jobExplorer-ref="jobExplorer" />

	<bean id="libraryListService"
		class="com.thomsonreuters.uscl.ereader.mgr.library.service.LibraryListServiceImpl"
		p:libraryListDao-ref="libraryListDao" />
		
	<bean id="materialComponentComparatorProvider" class="com.thomsonreuters.uscl.ereader.sap.comparsion.MaterialComponentComparatorProviderImpl">
		<constructor-arg ref="bookDefinitionService" />
	</bean>

	<bean id="editBookDefinitionService"
		class="com.thomsonreuters.uscl.ereader.mgr.web.controller.bookdefinition.edit.EditBookDefinitionServiceImpl">
		<property name="codeService" ref="codeService" />
		<property name="stateCodeService" ref="stateCodeService" />
		<property name="rootCodesWorkbenchLandingStrip" value="${codes.workbench.root.dir}" />
		<property name="frontMatterThemes">
			<list>
				<value>WestLaw Next</value>
				<value>AAJ Press</value>
			</list>
		</property>
		<property name="sapService" ref="sapService" />
		<property name="materialComponentComparatorProvider" ref="materialComponentComparatorProvider" />
	</bean>

	<bean id="miscConfigSyncService"
		class="com.thomsonreuters.uscl.ereader.mgr.web.service.ManagerMiscConfigSyncService"
		p:proviewClient-ref="proviewClient" p:httpClientFactory-ref="closeableAuthenticationHttpClientFactory"
		p:proviewRestTemplate-ref="proviewRestTemplate" />

	<bean id="managerAppConfigLoader"
		class="com.thomsonreuters.uscl.ereader.mgr.web.service.ManagerAppConfigLoader"
		p:appConfigService-ref="appConfigService" p:miscConfigSyncService-ref="miscConfigSyncService" />

	<bean id="managerService"
		class="com.thomsonreuters.uscl.ereader.mgr.web.service.ManagerServiceImpl"
		p:environmentName-ref="environmentName" p:generatorContextName="${generator.context.name}"
		p:rootWorkDirectory="${root.work.directory}"
		p:rootCodesWorkbenchLandingStrip="${codes.workbench.root.dir}"
		p:restTemplate-ref="generatorRestTemplate" p:managerDao-ref="managerDao"
		p:jobService-ref="jobService" p:jobRequestService-ref="jobRequestService" />
		
	<bean id="publishingStatsUtil"
		class="com.thomsonreuters.uscl.ereader.stats.util.PublishingStatsUtilImpl" />

	<bean id="xppBundleArchiveService"
		class="com.thomsonreuters.uscl.ereader.request.service.XppBundleArchiveService"
		p:xppBundleArchiveDao-ref="xppBundleArchiveDao" />

	<bean id="printComponentUtil"
		class="com.thomsonreuters.uscl.ereader.request.service.PrintComponentUtil" />

	<bean id="generatorRestClient"
		class="com.thomsonreuters.uscl.ereader.core.service.GeneratorRestClientImpl"
		p:restTemplate-ref="generatorRestTemplate" p:generatorContextUrl="${generator.context.url}" />

	<bean id="ssh" class="com.thomsonreuters.uscl.ereader.util.Ssh" />

	<bean id="emailNotification" class="com.thomsonreuters.uscl.ereader.util.EmailNotification" />

	<bean id="jobCleanupService"
		class="com.thomsonreuters.uscl.ereader.core.job.service.JobCleanupServiceImpl"
		p:jobCleanupDao-ref="jobCleanupDao" />

	<bean id="userPreferenceService"
		class="com.thomsonreuters.uscl.ereader.userpreference.service.UserPreferenceServiceImpl"
		p:userPreferenceDao-ref="userPreferenceDao" />

	<bean id="serverAccessService"
		class="com.thomsonreuters.uscl.ereader.core.job.service.ServerAccessServiceImpl"
		p:jobCleanupService-ref="jobCleanupService" />

	<bean id="outageService"
		class="com.thomsonreuters.uscl.ereader.core.outage.service.OutageServiceImpl"
		p:outageDao-ref="outageDao" />

	<bean id="supportPageLinkService"
		class="com.thomsonreuters.uscl.ereader.support.service.SupportPageLinkServiceImpl"
		p:supportPageLinkDao-ref="supportPageLinkDao" />

	<bean id="proviewTitleListService"
		class="com.thomsonreuters.uscl.ereader.mgr.web.controller.proviewlist.ProviewTitleListServiceImpl"
		p:bookDefinitionService-ref="bookDefinitionService"
		p:bookTitlesUtil-ref="bookTitlesUtil" />
</beans>